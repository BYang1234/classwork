   x = 40
    y  = 10
    x_end = 600
    wid = 20
    height_1 = 100
    for num in range(640):
        if x >= x_end:
            pass
        else:
          arcade.draw_rectangle_filled(x, y, wid, height_1, arcade.color.DARK_GREEN)
          arcade.draw_triangle_filled(x - 30, y + 50, x, y + 160, x + 30, y + 50, arcade.color.BROWN)
        x += 100
